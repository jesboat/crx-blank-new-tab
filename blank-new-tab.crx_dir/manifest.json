{
  "name": "Blank New Tab",
  "description": "Replaces the 'New Tab' page with a small, resource-efficient placeholder.",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png",
    "default_title": "'Blank New Tab' extension icon"
  },
  "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },
  "background": {
    "service_worker": "worker.js"
  },
  "icons": {
    "16": "icon-16x16.png",
    "32": "icon-32x32.png",
    "48": "icon-48x48.png",
    "128": "icon.png"
  },
  "short_name": "Blank New Tab"
}
